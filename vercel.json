{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "intake.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/intake", "dest": "/intake.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}


